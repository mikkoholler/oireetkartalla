{
  "name": "oireetkartalla",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": "1.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/types": "^5.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "firebase": "^7.12.0",
    "geokdbush": "^1.1.0",
    "google-map-react": "^1.1.7",
    "kdbush": "^3.0.0",
    "nodemon": "^2.0.2",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.0.1",
    "typescript": "~3.7.2"
  },
  "resolutions": {
    "@types/react-native": "link:./empty-package"
  },
  "scripts": {
    "tsc": "tsc",
    "server": "tsc server.ts --outDir dist && node dist/server.js",
    "start": "npm run server",
    "dev": "concurrently --kill-others --names server-build,server,frontend --prefix-colors black.bgGreen,black.bgBlue,black.bgYellow \"tsc -w --outDir dist server.ts\" \"nodemon ./dist/server.js\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "heroku-postbuild": "npm run build",
    "eject": "react-scripts eject",
    "prettier-check": "prettier --check --ignore-path .gitignore '**/*.{js,jsx,json,ts,tsx,css,scss,md}'",
    "prettier-write": "prettier --write --ignore-path .gitignore '**/*.{js,jsx,json,ts,tsx,css,scss,md}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/geokdbush": "^1.1.2",
    "@types/jest": "^24.0.0",
    "@types/kdbush": "^3.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.0.1",
    "@types/google-map-react": "^1.1.5",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.1",
    "ts-node": "^8.7.0"
  },
  "proxy": "http://localhost:5000"
}
